    "babel-preset-es2015": "6.1.2",
    "chai": "3.4.1",
    "eslint": "1.9.0",
    "eslint-config-dhis2": "0.0.5",
    "eslint-plugin-react": "3.8.0",
    "karma": "0.13.15",
    "karma-chai": "0.1.0",
    "karma-chrome-launcher": "0.2.1",
    "karma-cli": "0.1.1",
    "karma-coverage": "0.5.3",
