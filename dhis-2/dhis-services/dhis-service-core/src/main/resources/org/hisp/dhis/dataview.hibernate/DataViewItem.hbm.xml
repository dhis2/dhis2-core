<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="org.hisp.dhis.dataview.DataViewItem" table="dataviewitem">

        <id name="id" column="dataviewitemid">
            <generator class="native" />
        </id>

        <many-to-one name="dataView" class="org.hisp.dhis.dataview.DataView" column="dataviewid"
                     foreign-key="fk_dataviewitem_dataviewid" unique-key="dataviewitem_unique_key" not-null="true" />

        <many-to-one name="trackedEntityAttribute" class="org.hisp.dhis.trackedentity.TrackedEntityAttribute"
                     column="trackedentityattributeeid"
                     foreign-key="fk_dataviewitem_trackedentityattributeid" />

        <many-to-one name="dataElement" class="org.hisp.dhis.dataelement.DataElement"
                     column="dataelementid"
                     foreign-key="fk_dataviewitem_dataelementid" />

    </class>

</hibernate-mapping>