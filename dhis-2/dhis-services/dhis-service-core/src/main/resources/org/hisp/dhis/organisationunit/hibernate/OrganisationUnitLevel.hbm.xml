<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"
  [ <!ENTITY  identifiableProperties SYSTEM "classpath://org/hisp/dhis/common/identifiableProperties.hbm"> 
 <!ENTITY  tablename "orgunitlevel">] 
  >

<hibernate-mapping>
  <class name="org.hisp.dhis.organisationunit.OrganisationUnitLevel" table="&tablename;">

    <cache usage="read-write" />

    <id name="id" column="orgunitlevelid">
      <generator class="native" />
    </id>
    &identifiableProperties;

    <property name="name" not-null="true" length="230" />

    <property name="level" not-null="true" unique-key="uk_level_orgunitlevel" />

    <property name="offlineLevels" />

    <property name="translations" type="jblTranslations"/>

  </class>

</hibernate-mapping>
