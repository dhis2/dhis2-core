<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"
        [<!ENTITY identifiableProperties SYSTEM "classpath://org/hisp/dhis/common/identifiableProperties.hbm">]
        >

<hibernate-mapping>
    <class name="org.hisp.dhis.icon.Icon" table="icon">

        <id name="id" column="icon">
            <generator class="native" />
        </id>
        &identifiableProperties;

        <property name="key" column="iconkey" not-null="true" unique="true" length="100" />
        <property name="description" column="description"  length="200" />
        <property name="custom" column="custom" />
        <property name="keywords" column="keywords" type="jbSet" />

        <many-to-one name="fileResource" column="fileresourceid" cascade="all-delete-orphan" lazy="false" unique="true" not-null="false"/>

        <many-to-one name="createdBy" class="org.hisp.dhis.user.User" column="createdby" foreign-key="fk_createdby_userid" />

    </class>

</hibernate-mapping>
