<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="org.hisp.dhis.trackerdataview.TrackerDataView" table="trackerdataview">
        <id name="id" column="trackerdataviewid">
            <generator class="native" />
        </id>

        <many-to-one name="trackedEntityType" class="org.hisp.dhis.trackedentity.TrackedEntityType"
                     column="trackedentitytypeid"
                     foreign-key="fk_trackerdataview_trackedentitytypeid" />

        <many-to-one name="program" class="org.hisp.dhis.program.Program"
                     column="programid"
                     foreign-key="fk_trackerdataview_programid" />

        <many-to-one name="programStage" class="org.hisp.dhis.program.ProgramStage"
                     column="programstageid"
                     foreign-key="fk_trackerdataview_programstageid" />

    </class>

</hibernate-mapping>