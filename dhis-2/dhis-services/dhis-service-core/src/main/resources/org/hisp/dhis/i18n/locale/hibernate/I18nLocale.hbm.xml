<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"
    [ <!ENTITY  identifiableProperties SYSTEM "classpath://org/hisp/dhis/common/identifiableProperties.hbm"> 
 <!ENTITY  tablename "i18nlocale">] 
    >

<hibernate-mapping>
  <class name="org.hisp.dhis.i18n.locale.I18nLocale" table="&tablename;">

    <id name="id" column="i18nlocaleid">
      <generator class="native" />
    </id>
    &identifiableProperties;

    <property name="name" column="name" length="250" not-null="true" unique-key="uk_name_&tablename;" />

    <property name="locale" column="locale" length="15" not-null="true" unique-key="uk_locale_&tablename;" />

  </class>
</hibernate-mapping>
